# Server Configuration
NODE_ENV=development
PORT=5000
FRONTEND_URL=http://localhost:3000

# Firebase Configuration
FIREBASE_TYPE=service_account
FIREBASE_PROJECT_ID=smartagrinet-africa
FIREBASE_PRIVATE_KEY_ID=your_private_key_id
FIREBASE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nYour private key here\n-----END PRIVATE KEY-----\n"
FIREBASE_CLIENT_EMAIL=firebase-adminsdk-xxxxx@smartagrinet-africa.iam.gserviceaccount.com
FIREBASE_CLIENT_ID=your_client_id
FIREBASE_AUTH_URI=https://accounts.google.com/o/oauth2/auth
FIREBASE_TOKEN_URI=https://oauth2.googleapis.com/token
FIREBASE_AUTH_PROVIDER_X509_CERT_URL=https://www.googleapis.com/oauth2/v1/certs
FIREBASE_CLIENT_X509_CERT_URL=https://www.googleapis.com/robot/v1/metadata/x509/firebase-adminsdk-xxxxx%40smartagrinet-africa.iam.gserviceaccount.com

# Firebase Client SDK
FIREBASE_API_KEY=your_api_key
FIREBASE_AUTH_DOMAIN=smartagrinet-africa.firebaseapp.com
FIREBASE_STORAGE_BUCKET=smartagrinet-africa.appspot.com
FIREBASE_MESSAGING_SENDER_ID=your_sender_id
FIREBASE_APP_ID=your_app_id
FIREBASE_MEASUREMENT_ID=your_measurement_id

# JWT Configuration
JWT_SECRET=your_jwt_secret_key_here
JWT_EXPIRES_IN=7d

# Database Configuration
DATABASE_URL=your_database_url

# Email Configuration (SendGrid)
SENDGRID_API_KEY=your_sendgrid_api_key
SENDGRID_FROM_EMAIL=noreply@smartagrinet.com
SENDGRID_FROM_NAME=SmartAgriNet

# SMS Configuration (Twilio)
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=+1234567890

# Weather API Configuration
OPENWEATHER_API_KEY=your_openweather_api_key
WEATHER_API_BASE_URL=https://api.openweathermap.org/data/2.5

# Google Cloud Vision API
GOOGLE_CLOUD_PROJECT_ID=smartagrinet-africa
GOOGLE_CLOUD_VISION_API_KEY=your_google_cloud_vision_api_key

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key
OPENAI_MODEL=gpt-4

# Maps API Configuration
GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Payment Gateway Configuration (Stripe)
STRIPE_SECRET_KEY=your_stripe_secret_key
STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret

# Banking API Configuration
BANKING_API_BASE_URL=https://api.banking-partner.com
BANKING_API_KEY=your_banking_api_key

# Insurance API Configuration
INSURANCE_API_BASE_URL=https://api.insurance-partner.com
INSURANCE_API_KEY=your_insurance_api_key

# Government API Configuration
GOVERNMENT_API_BASE_URL=https://api.government.gov
GOVERNMENT_API_KEY=your_government_api_key

# Satellite Imagery API
SATELLITE_API_BASE_URL=https://api.satellite-provider.com
SATELLITE_API_KEY=your_satellite_api_key

# Supply Chain API
SUPPLY_CHAIN_API_BASE_URL=https://api.supply-chain.com
SUPPLY_CHAIN_API_KEY=your_supply_chain_api_key

# Logging Configuration
LOG_LEVEL=info
LOG_FILE_PATH=./logs

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# File Upload Configuration
MAX_FILE_SIZE=10485760
ALLOWED_FILE_TYPES=image/jpeg,image/png,image/webp

# CORS Configuration
CORS_ORIGIN=http://localhost:3000,https://smartagrinet.com

# Redis Configuration (for caching and sessions)
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=your_redis_password

# AWS Configuration (for file storage backup)
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=us-east-1
AWS_S3_BUCKET=smartagrinet-files

# Monitoring and Analytics
SENTRY_DSN=your_sentry_dsn
ANALYTICS_API_KEY=your_analytics_api_key

# Security Configuration
BCRYPT_ROUNDS=12
SESSION_SECRET=your_session_secret

# Feature Flags
ENABLE_AI_FEATURES=true
ENABLE_OFFLINE_SYNC=true
ENABLE_PUSH_NOTIFICATIONS=true
ENABLE_VOICE_COMMANDS=true

# Multi-language Support
DEFAULT_LANGUAGE=en
SUPPORTED_LANGUAGES=en,fr,sw,ha,yo,ar

# Crop Recommendation Model
CROP_MODEL_PATH=./ai-models/crop_recommendation_model.json
SOIL_ANALYSIS_MODEL_PATH=./ai-models/soil_analysis_model.json
PEST_DETECTION_MODEL_PATH=./ai-models/pest_detection_model.tflite

# Irrigation System Configuration
IRRIGATION_API_BASE_URL=https://api.irrigation-system.com
IRRIGATION_API_KEY=your_irrigation_api_key

# Livestock Management
LIVESTOCK_API_BASE_URL=https://api.livestock-management.com
LIVESTOCK_API_KEY=your_livestock_api_key

# Marketplace Configuration
MARKETPLACE_COMMISSION_RATE=0.05
MARKETPLACE_MIN_TRANSACTION_AMOUNT=1.00

# Notification Configuration
PUSH_NOTIFICATION_SERVER_KEY=your_push_notification_server_key
EMAIL_NOTIFICATIONS_ENABLED=true
SMS_NOTIFICATIONS_ENABLED=true
PUSH_NOTIFICATIONS_ENABLED=true

# Backup Configuration
BACKUP_ENABLED=true
BACKUP_SCHEDULE=0 2 * * *
BACKUP_RETENTION_DAYS=30

# Development Configuration
DEBUG_MODE=true
ENABLE_SWAGGER=true
ENABLE_LOGGING=true 